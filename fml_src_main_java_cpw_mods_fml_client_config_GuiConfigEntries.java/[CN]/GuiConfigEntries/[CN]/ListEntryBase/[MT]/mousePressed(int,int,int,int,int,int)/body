{
  if (this.btnDefault.mousePressed(this.mc,x,y)) {
    btnDefault.func_146113_a(mc.getSoundHandler());
    setToDefault();
    return true;
  }
 else   if (this.btnUndoChanges.mousePressed(this.mc,x,y)) {
    btnUndoChanges.func_146113_a(mc.getSoundHandler());
    undoChanges();
    return true;
  }
  return false;
}
