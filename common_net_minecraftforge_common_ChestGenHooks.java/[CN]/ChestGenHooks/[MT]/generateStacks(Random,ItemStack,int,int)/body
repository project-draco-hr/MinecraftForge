{
  int count=min + (rand.nextInt(max - min + 1));
  ItemStack[] ret;
  if (source.getItem() == null) {
    ret=new ItemStack[0];
  }
 else   if (count > source.getMaxStackSize()) {
    ret=new ItemStack[count];
    for (int x=0; x < count; x++) {
      ret[x]=source.copy();
      ret[x].stackSize=1;
    }
  }
 else {
    ret=new ItemStack[1];
    ret[0]=source.copy();
    ret[0].stackSize=count;
  }
  return ret;
}
