{
  if (RelaunchLibraryManager.downloadMonitor != null) {
    return;
  }
  try {
    RelaunchLibraryManager.downloadMonitor=new Downloader();
    if (showIt) {
      popupWindow=RelaunchLibraryManager.downloadMonitor.makeDialog();
    }
  }
 catch (  Exception e) {
    Downloader.makeHeadless();
    popupWindow=null;
  }
}
