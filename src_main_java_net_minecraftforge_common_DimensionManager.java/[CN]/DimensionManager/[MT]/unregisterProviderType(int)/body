{
  if (!providers.containsKey(id)) {
    return new int[0];
  }
  providers.remove(id);
  spawnSettings.remove(id);
  int[] ret=new int[dimensions.size()];
  int x=0;
  for (  Map.Entry<Integer,Integer> ent : dimensions.entrySet()) {
    if (ent.getValue() == id) {
      ret[x++]=ent.getKey();
    }
  }
  return Arrays.copyOf(ret,x);
}
