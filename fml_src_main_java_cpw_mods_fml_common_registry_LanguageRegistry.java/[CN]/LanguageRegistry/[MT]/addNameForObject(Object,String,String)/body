{
  String objectName;
  if (objectToName instanceof Item) {
    objectName=((Item)objectToName).func_77658_a();
  }
 else   if (objectToName instanceof Block) {
    objectName=((Block)objectToName).func_71917_a();
  }
 else   if (objectToName instanceof ItemStack) {
