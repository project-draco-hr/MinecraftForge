{
  nbt.setInteger("Amount",amount);
  nbt.setShort("Id",(short)itemID);
  nbt.setShort("Meta",(short)itemMeta);
  String name=LiquidDictionary.findLiquidName(this);
  if (name != null) {
    nbt.setString("LiquidName",name);
  }
  if (extra != null) {
    nbt.setTag("extra",extra);
  }
  return nbt;
}
