{
  if (keyHandlers == null) {
    keyHandlers=new ArrayList<IKeyHandler>();
  }
  Iterator<IKeyHandler> itr=keyHandlers.iterator();
  while (itr.hasNext()) {
    IKeyHandler old=itr.next();
    if (old.getKeyBinding() == handler.getKeyBinding()) {
      itr.remove();
    }
  }
  keyHandlers.add(handler);
}
