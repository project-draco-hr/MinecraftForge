def load_suggestions(fml_dir):
    map_file = os.path.join(fml_dir, 'map_client.txt')
    if (not os.path.isfile(map_file)):
        return None
    map = {}
    with closing(open(map_file, 'r')) as fh:
        for line in fh:
            if (line[0] == ' '):
                continue
            line = line[0:(-2)].split(' -> ')
            map[line[1]] = line[0].rsplit('.', 1)[0]
    return map
