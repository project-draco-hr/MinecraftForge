  addDiscriminator((byte)3,FMLHandshakeMessage.RegistryData.class);
  addDiscriminator((byte)-1,FMLHandshakeMessage.HandshakeAck.class);
  addDiscriminator((byte)-2,FMLHandshakeMessage.HandshakeReset.class);
}
