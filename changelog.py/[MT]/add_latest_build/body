def add_latest_build(url, builds, current_version=None):
    data = read_url(url)
    data = ast.literal_eval(data)
    number = data['number']
    if (builds[0]['number'] == data['number']):
        return builds
    for item in data['changeSet']['items']:
        item['author'] = item['author']['fullName']
    build = {'number': data['number'], 'result': 'SUCCESS', 'version': current_version, 'items': data['changeSet']['items'], }
    return ([build] + builds)
