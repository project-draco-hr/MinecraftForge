{
  for (  VertexFormatElement e : (List<VertexFormatElement>)format.getElements()) {
switch (e.getUsage()) {
case POSITION:
      put(buf,e,x,y,z,1f);
    break;
case COLOR:
  put(buf,e,1f,1f,1f,1f);
break;
case UV:
put(buf,e,u,v,0f,1f);
break;
case NORMAL:
put(buf,e,(float)side.getFrontOffsetX(),(float)side.getFrontOffsetY(),(float)side.getFrontOffsetZ(),0f);
break;
default :
put(buf,e);
break;
}
}
}
