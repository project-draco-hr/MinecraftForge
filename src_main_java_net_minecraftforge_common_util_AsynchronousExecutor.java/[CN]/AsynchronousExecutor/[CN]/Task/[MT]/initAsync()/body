{
  if (STATE.compareAndSet(this,PENDING,STAGE_1_ASYNC)) {
    boolean ret=true;
    try {
      init();
    }
  finally {
      if (STATE.compareAndSet(this,STAGE_1_ASYNC,STAGE_1_COMPLETE)) {
      }
 else {
synchronized (this) {
          if (state != STAGE_1_SYNC) {
            this.notifyAll();
          }
 else {
          }
          state=STAGE_1_COMPLETE;
        }
        ret=false;
      }
    }
    return ret;
  }
 else {
    return false;
  }
}
