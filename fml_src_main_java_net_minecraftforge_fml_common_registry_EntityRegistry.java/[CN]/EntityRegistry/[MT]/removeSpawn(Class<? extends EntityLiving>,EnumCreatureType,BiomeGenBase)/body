{
  for (  BiomeGenBase biome : biomes) {
    @SuppressWarnings("unchecked") Iterator<SpawnListEntry> spawns=biome.getSpawnableList(typeOfCreature).iterator();
    while (spawns.hasNext()) {
      SpawnListEntry entry=spawns.next();
      if (entry.entityClass == entityClass) {
        spawns.remove();
      }
    }
  }
}
