{
  Map<ModContainer,URL> ret=new HashMap<ModContainer,URL>();
  for (  ModContainer mod : Loader.instance().getActiveModList()) {
    URL url=null;
    try {
      url=mod.getUpdateUrl();
    }
 catch (    AbstractMethodError abs) {
    }
    if (url != null)     ret.put(mod,url);
  }
  return ret;
}
