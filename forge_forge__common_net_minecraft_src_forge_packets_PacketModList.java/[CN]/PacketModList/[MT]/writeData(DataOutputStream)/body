{
  if (!isServer) {
    data.writeInt(Mods.length);
    for (    String mod : Mods) {
      data.writeUTF(mod);
    }
  }
 else {
    data.writeInt(ModIDs.size());
    for (    Entry<Integer,String> entry : ModIDs.entrySet()) {
      data.writeInt(entry.getKey());
      data.writeUTF(entry.getValue());
    }
  }
}
