{
  if (isServer) {
    Length=data.readInt();
    if (Length >= 0) {
      Mods=new String[Length];
      for (int x=0; x < Length; x++) {
        Mods[x]=data.readUTF();
      }
    }
  }
 else {
    Length=data.readInt();
    for (int x=0; x < Length; x++) {
      ModIDs.put(data.readInt(),data.readUTF());
    }
  }
  try {
    has4096=data.readBoolean();
  }
 catch (  EOFException e) {
    has4096=false;
  }
}
