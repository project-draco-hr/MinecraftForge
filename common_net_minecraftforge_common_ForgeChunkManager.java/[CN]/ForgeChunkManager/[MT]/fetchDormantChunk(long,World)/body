{
  Cache<Long,Chunk> cache=dormantChunkCache.get(world);
  if (cache == null) {
    return null;
  }
  Chunk chunk=cache.getIfPresent(coords);
  if (chunk != null) {
    for (    List<Entity> eList : chunk.entityLists) {
      for (      Entity e : eList) {
        e.resetEntityId();
      }
    }
  }
  return chunk;
}
