{
  if ((version == Opcodes.V1_8 && !SystemUtils.isJavaVersionAtLeast(JavaVersion.JAVA_1_8)) || (version == Opcodes.V1_7 && !SystemUtils.isJavaVersionAtLeast(JavaVersion.JAVA_1_7))) {
    if (classMap.containsKey(name))     blame(classMap.get(name),name);
 else     orphanNaughtyClasses.add(name);
  }
}
