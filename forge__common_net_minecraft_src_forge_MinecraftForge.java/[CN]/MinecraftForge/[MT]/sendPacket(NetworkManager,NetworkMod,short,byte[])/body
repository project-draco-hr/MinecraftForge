{
  if (data == null) {
    data=new byte[0];
  }
  if (data.length > 255) {
    throw new IllegalArgumentException(String.format("Data argument was to long, must not be longer then 255 bytes was %d",data.length));
  }
  Packet131MapData pkt=new Packet131MapData();
  pkt.itemID=(short)getModID(mod);
  pkt.uniqueID=id;
  pkt.itemData=data;
  sendPacket(net,pkt);
}
