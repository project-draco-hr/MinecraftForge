{
  ItemStack is=customItemStacks.get(modId,name);
  if (is == null) {
    Item i=findItem(modId,name);
    if (i != null) {
      is=new ItemStack(i,0,0);
    }
  }
  if (is == null) {
    Block b=findBlock(modId,name);
    if (b != null) {
      is=new ItemStack(b,0,Short.MAX_VALUE);
    }
  }
  return is;
}
