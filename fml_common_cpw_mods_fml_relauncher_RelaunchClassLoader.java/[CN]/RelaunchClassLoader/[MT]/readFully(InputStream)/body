{
  try {
    ByteArrayOutputStream bos=new ByteArrayOutputStream(stream.available());
    int r;
    while ((r=stream.read()) != -1) {
      bos.write(r);
    }
    return bos.toByteArray();
  }
 catch (  Throwable t) {
    FMLLog.log(Level.WARNING,t,"Problem loading class");
    return new byte[0];
  }
}
