def get_headers(url):


    class HeadRequest(urllib2.Request):

        def get_method(self):
            return 'HEAD'
    response = urllib2.urlopen(HeadRequest(url))
    array = [line.rstrip('\r\n') for line in response.info().headers]
    dict = {}
    for line in array:
        pts = line.split(':', 1)
        pts[1] = pts[1].strip()
        if ((pts[1][0] == '"') and (pts[1][(-1)] == '"')):
            pts[1] = pts[1][1:(-1)]
        dict[pts[0]] = pts[1]
    return dict
