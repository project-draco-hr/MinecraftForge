{
  float maxRarity=0f;
  for (  ObjectPair<Float,String> mob : dungeonMobs) {
    maxRarity+=mob.getValue1();
  }
  float targetRarity=rand.nextFloat() * maxRarity;
  for (  ObjectPair<Float,String> mob : dungeonMobs) {
    if (targetRarity < mob.getValue1()) {
      return mob.getValue2();
    }
    targetRarity-=mob.getValue1();
  }
  return "";
}
